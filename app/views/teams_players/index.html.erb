<p id="notice"><%= notice %></p>

<h1>Listing Teams Players</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @teams_players.each do |teams_player| %>
      <tr>
        <td><%= link_to 'Show', teams_player %></td>
        <td><%= link_to 'Edit', edit_teams_player_path(teams_player) %></td>
        <td><%= link_to 'Destroy', teams_player, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Teams player', new_teams_player_path %>
